<html lang="en">
  <head>
    <meta charset="utf-8">

    <title>Pinteresting</title>
    <link href="https://fonts.googleapis.com/css?family=Lato:400,900|Rokkitt:400,700" rel="stylesheet">
    <%= stylesheet_link_tag "application.css" %>
  </head>

  <body>

  <div class="container">
    <%= form_for :article, url: articles_path do |f| %>
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>

    <p>
      <%= f.label :text %><br>
      <%= f.text_area :text %>
    </p>

    <p>
      <%= f.submit %>
    </p>

    <% end %>
  </div>

  </body>
</html>